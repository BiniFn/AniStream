{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "core:window:allow-center",
    "core:window:allow-set-size",
    "core:window:allow-set-min-size"
  ],
  "remote": {
    "urls": [
      "https://aniways.xyz",
      "http://localhost:3000",
      "http://localhost:8080",
      "https://api.aniways.xyz"
    ],
    "secure": false
  }
}
